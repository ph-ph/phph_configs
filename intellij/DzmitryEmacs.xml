<keymap version="1" name="Dzmitry Emacs" parent="Emacs">
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl y" />
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="EditorKillRegion">
    <keyboard-shortcut first-keystroke="ctrl w" />
    <keyboard-shortcut first-keystroke="meta x" />
  </action>
  <action id="EditorKillRingSave">
    <keyboard-shortcut first-keystroke="alt w" />
    <keyboard-shortcut first-keystroke="meta c" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl alt w" />
    <keyboard-shortcut first-keystroke="ctrl equals" />
  </action>
  <action id="ExpandAll">
    <keyboard-shortcut first-keystroke="ctrl add" />
  </action>
  <action id="ExpandExpandableComponent">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl add" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl add" />
  </action>
  <action id="ExternalSystem.ExpandAll">
    <keyboard-shortcut first-keystroke="ctrl add" />
  </action>
  <action id="GotoLine">
    <keyboard-shortcut first-keystroke="alt g" />
    <keyboard-shortcut first-keystroke="meta g" />
  </action>
  <action id="Images.Editor.ZoomIn">
    <keyboard-shortcut first-keystroke="ctrl add" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="n" />
    <keyboard-shortcut first-keystroke="ctrl tab" />
  </action>
  <action id="PasteMultiple">
    <keyboard-shortcut first-keystroke="alt y" />
    <keyboard-shortcut first-keystroke="meta y" />
  </action>
</keymap>